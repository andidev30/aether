PORT=3000

# Elastic Cloud
ELASTIC_URL=https://your-elastic-cluster.es.us-central1.gcp.elastic.cloud:443
ELASTIC_API_KEY=base64encodedApiKey
ELASTIC_INDEX=aether
ELASTIC_RESULT_SIZE=5
AUDIT_SOURCE_CONFIDENCE_THRESHOLD=0.75
# Optional TLS certificate path
# ELASTIC_CA_CERT_PATH=/path/to/http_ca.crt

# Google Vertex AI
VERTEX_PROJECT_ID=your-gcp-project
VERTEX_LOCATION=us-central1
VERTEX_EMBEDDING_MODEL=text-embedding-005
VERTEX_MODERATION_MODEL=text-moderation-latest
# Provide GOOGLE_APPLICATION_CREDENTIALS pointing to a service account JSON file

# Gemini (Vertex-hosted generative model)
GEMINI_MODEL=gemini-1.5-flash

# Optional HTTP timeout override (milliseconds)
# HTTP_TIMEOUT_MS=12000
